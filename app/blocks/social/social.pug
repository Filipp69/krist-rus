mixin social
    .social
        a.social__item(href="#" target="_blank")
            svg.social__icon(width="22" height="12")
                use(href='#icon-vk')
        a.social__item(href="#" target="_blank")
            svg.social__icon(width="18" height="15")
                use(href='#icon-telegram')
        a.social__item(href="#" target="_blank")
            svg.social__icon(width="19" height="15")
                use(href='#icon-youtube')
